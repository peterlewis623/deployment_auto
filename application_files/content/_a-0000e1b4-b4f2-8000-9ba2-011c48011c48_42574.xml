    <versionUuid>_a-0000e1b4-b4f2-8000-9ba2-011c48011c48_42651</versionUuid>
    <rule>
        <name>ICM_AbsencesComponent</name>
        <uuid>_a-0000e1b4-b4f2-8000-9ba2-011c48011c48_42574</uuid>
        <description>Expression to work with the absences in week expression rule</description>
        <parentUuid>_a-0000dd1c-f866-8000-aae2-01ef9001ef90_3578</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>ri!day &amp; ": " &amp; joinarray(#"SYSTEM_SYSRULES_forEach"(
  items: ri!days,
  expression: if(
    like(fv!item,ri!day),
    ri!people[fv!index],
    null
    )
),", ")</definition>
        <namedTypedValue>
            <name>days</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>people</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>day</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
