<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e046-0511-8000-9ba2-011c48011c48_942787</versionUuid>
    <rule>
        <name>ICM_IllegalUsernameCharacters</name>
        <uuid>6fa0d87c-dab6-424a-97a3-a2629f95f7f3</uuid>
        <description>Returns a true is a first or last name contains an illegal character</description>
        <parentUuid>_a-0000dd1c-f866-8000-aae2-01ef9001ef90_3578</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  isnull(ri!name),
  false,
  with(
  local!illegalOperators:{"!.#.$.%.^.&amp;.*.(.).+.=.|.\.;.&lt;.,.&gt;./.?.{.}.[.].`.~." &amp; char(34)},
  local!splitIllegalOperators:split(local!illegalOperators,"."),
  local!value: apply(
      char(_),
      code(ri!name)
      ),
  or(
    apply(
    contains(
      local!value,_
    ),
    local!splitIllegalOperators
  )
  )
)
)</definition>
        <namedTypedValue>
            <name>name</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
